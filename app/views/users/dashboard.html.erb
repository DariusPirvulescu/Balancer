<h1>Hello, <%= current_user.first_name %>! These are your transactions:  <h1>

<ol>
  <% @transactions.each do |tran| %>
    <li><%= tran.id %></li>
  <% end %>
</ol>

<h1>Your projects:<h1>

<ol>
  <% @current_user.projects.each do |prj| %>
    <li>
      <%= prj.project_name %>
      <% prj.campaigns.each do |cmp| %>
        <li><%= cmp.id %></li>
      <% end %>
    </li>
  <% end %>
</ol>
